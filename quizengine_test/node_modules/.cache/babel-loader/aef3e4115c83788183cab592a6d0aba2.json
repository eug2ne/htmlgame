ul>\n\n  <div id=\"clue-board\">\n    <div class=\"clue\" v-for=\"clue in this.show\" :key=\"clue.id\">\n      <div class=\"cluewrapper\" v-if=\"clue.achieve\">\n        <h3>{{ clue.title }}</h3>\n        <p><a v-if=\"clue.a\" :href=\"clue.a\">{{ clue.descript }}</a></p>\n      </div>\n\n      <div class=\"cluewrapper\" v-else>\n        <h3>아직 얻지못한 단서입니다</h3>\n      </div>\n\n      <div class=\"subclue\" v-for=\"subclue in clue.subclue\" :key=\"subclue.id\">\n        <div class=\"subcluewrapper\" v-if=\"subclue.achieve\">\n          <h3>{{ subclue.title }}</h3>\n          <p>{{ subclue.descript }}</p>\n        </div>\n\n        <div class=\"subcluewrapper\" v-else>\n          <h3>아직 잠겨있습니다</h3>\n          <p>\n            <router-link :to=\"subclue.link\">\n              퍼즐 풀고 단서 얻으러가기\n            </router-link>\n          </p>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { ref } from 'vue'\n\nexport default {\n  data() {\n    return {\n      cluelist: ref([]),\n      show: ref([])\n    }\n  },\n  beforeMount() {\n    // fetch('http://localhost:3000/cluelist')\n    //   .then(response => response.json())\n    //   .then(data => this.cluelist = data)\n    //   .catch(error => console.log(error.message))\n  },\n  methods: {\n    showClue(story) {\n      this.show = story.clues\n    }\n  }\n}\n</script>\n\n<style scoped>\nul#timeline {\n  width: 150px;\n  display: inline-block;\n}\n\ndiv#clue-board {\n  width: 400px;\n  height: 500px;\n  display: inline-block;\n}\n\ndiv.clue {\n  width: 250px;\n  height: 200px;\n  margin-bottom: 20px;\n  display: flex;\n  flex-wrap: wrap;\n  flex-direction: row;\n}\n</style>"],"sourceRoot":""}]}